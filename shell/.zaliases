# Aliases
alias vi='vim'
alias ls='ls -G'
alias l='ls -CF'
alias la='ls -A'
alias ll='ls -l'
alias grep='grep --colour'
alias ifind='find . -iname $1'
alias igrep='grep -nrif /dev/stdin . <<<'
alias gitfr='git fetch && git rebase'
alias gitac='git add -u && git commit --amend'
alias text='open -a TextEdit'
alias cdua='/Volumes/git/usageanalytics'
alias cdpacman='/Volumes/git/ms-pacman'
alias cdadonis='/Volumes/git/adonis'
alias cdcode='/Volumes/git'
alias cdcompass='/Volumes/git/compass'
alias cdcontour='/Volumes/git/contour'
alias cdvoltron='/Volumes/git/voltron'
alias cdworkspace='/Volumes/git/workspace-server'
alias cdanalysis='/Volumes/git/analysis'
alias gwall='./gradlew clean cleanEclipse build eclipse -parallel-threads 8'
alias gwcleanbuild='./gradlew clean cleanEclipse build -parallel-threads 8'
alias gwcleaneclipse='./gradlew clean cleanEclipse eclipse -parallel-threads 8'
alias gwbuild='./gradlew build -parallel-threads 8'
alias gweclipse='./gradlew eclipse -parallel-threads 8'
alias gwbuildeclipse='./gradlew build eclipse -parallel-threads 8'
alias exlif='python ~/exif_reader.py $1'
alias chrome='open -a "/Applications/Google Chrome.app" $1'
alias server='python -m SimpleHTTPServer'
alias dev-ete='docker-compose -f docker-compose.ete.yml'
alias docker-stop-and-rm='docker stop $(docker ps -a -q) && docker rm -vf $(docker ps -a -q)'
alias dev-contour='docker-compose -f docker-compose.contour-local.yml'
alias portfix='pkill com.docker.slir'
